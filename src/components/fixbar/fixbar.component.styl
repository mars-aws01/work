.nk-fixbar {
  position: fixed;
  right: 15px;
  bottom: 117px;
  z-index: 9999;

  li {
    list-style: none;
    background: #393D49;
    width: 50px;
    height: 50px;
    line-height: 50px;
    margin-bottom: 1px;
    color: #fff;
    border-radius: 2px;
    opacity: 0.95;
    text-align: center;
    cursor: pointer;
    font-size: 20px;

    &:hover {
      opacity: 0.85;
    }
  }
}
