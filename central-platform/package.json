{
  "name": "@newkit/central-platform-web",
  "version": "1.8.4",
  "description": "The modular web platform based Angular.",
  "main": "index.html",
  "types": "./src/declare.d.ts",
  "scripts": {
    "vendor": "gulp4 build:vendor --gulpfile build/gulpfile.js --cwd ./",
    "vendor:prod": "gulp4 build:vendor --gulpfile build/gulpfile.js --cwd ./ --minify",
    "ext": "webpack --config build/webpack.external.conf",
    "ext:prod": "cross-env NODE_ENV=production webpack --config build/webpack.external.conf",
    "types:newkit": "gulp4 types:newkit --gulpfile build/gulpfile.js --cwd ./",
    "types": "gulp4 types:modules --gulpfile build/gulpfile.js --cwd ./",
    "dev": "npm run vendor && gulp4 dev --gulpfile build/gulpfile.js --cwd ./",
    "build": "gulp4 build --gulpfile build/gulpfile.js --cwd ./ --minify --prod && npm run vendor:prod",
    "start": "gulp4 --gulpfile newkit_package/build/client-gulpfile.js --cwd ./",
    "test:e2e": "testcafe chrome test/e2e || true",
    "test:unit": "echo 'Not supported.'",
    "test": "npm run test:e2e & npm run test:unit"
  },
  "repository": {
    "type": "git",
    "url": "http://trgit2/newkit/central-platform.git"
  },
  "keywords": [
    "Newkit",
    "Angular2"
  ],
  "author": "BTS Team",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "4.4.3",
    "@angular/common": "4.4.3",
    "@angular/compiler": "4.4.3",
    "@angular/core": "4.4.3",
    "@angular/forms": "4.4.3",
    "@angular/http": "4.4.3",
    "@angular/platform-browser": "4.4.3",
    "@angular/platform-browser-dynamic": "4.4.3",
    "@angular/router": "4.4.3",
    "animate.css": "3.5.2",
    "core-js": "2.5.1",
    "echarts": "3.7.1",
    "font-awesome": "4.7.0",
    "hammerjs": "^2.0.8",
    "jquery": "2.2.4",
    "jszip": "3.1.4",
    "loaders.css": "0.1.2",
    "lodash": "4.17.4",
    "moment": "2.18.1",
    "nprogress": "0.2.0",
    "reflect-metadata": "0.1.10",
    "rxjs": "5.4.3",
    "zone.js": "0.8.17"
  },
  "devDependencies": {
    "@newkit/core": "^0.1.4",
    "@ngx-translate/core": "^8.0.0",
    "@types/node": "^8.0.24",
    "angular2-template-loader": "0.6.2",
    "awesome-typescript-loader": "3.2.3",
    "babel-eslint": "8.0.0",
    "bootstrap": "^3.3.7",
    "browser-sync": "2.18.13",
    "connect-history-api-fallback": "1.3.0",
    "cross-env": "^5.0.5",
    "css-loader": "0.28.7",
    "extract-text-webpack-plugin": "^3.0.0",
    "gulp-concat": "2.6.1",
    "gulp-newer": "1.3.0",
    "gulp-uglify": "3.0.0",
    "gulp-util": "3.0.8",
    "gulp4": "4.0.2",
    "less": "2.7.2",
    "less-loader": "4.0.5",
    "moment-timezone": "^0.5.13",
    "ngx-ckeditor": "^0.1.1",
    "ngx-smartadmin": "^0.3.0",
    "node-notifier": "5.1.2",
    "node-sass": "^4.5.3",
    "raw-loader": "0.5.1",
    "sass-loader": "6.0.6",
    "shelljs": "0.7.8",
    "stylus": "0.54.5",
