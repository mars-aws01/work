{
  "name": "newkit-vendors",
  "version": "0.4.3",
  "description": "Pack some vendor libs fornewkit, like polyfills、angular、kendo/wijmo and so on",
  "main": "index.js",
  "scripts": {
    "vendor:angular": "gulp4 angular --gulpfile build/gulpfile.js --cwd ./",
    "webpack:kw": "webpack --config build/webpack.base.conf",
    "webpack:kw-min": "cross-env NODE_ENV=production webpack --config build/webpack.base.conf",
    "vendor:kw": "gulp4 kw --gulpfile build/gulpfile.js --cwd ./",
    "vendor": "gulp4 vendor --gulpfile build/gulpfile.js --cwd ./ --color=always"
  },
  "repository": {
    "type": "git",
    "url": "http://trgit2/newkit/newkit-vendors.git"
  },
  "keywords": [
    "Newkit",
    "Vendor",
    "polyfills",
    "angular",
    "kendo",
    "wijmo"
  ],
  "author": "Jay.M.Hu",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "4.4.6",
    "@angular/common": "4.4.6",
    "@angular/compiler": "4.4.6",
    "@angular/core": "4.4.6",
    "@angular/forms": "4.4.6",
    "@angular/http": "4.4.6",
    "@angular/platform-browser": "4.4.6",
    "@angular/platform-browser-dynamic": "4.4.6",
    "@angular/router": "4.4.6",
    "@progress/kendo-angular-buttons": "^2.0.1",
    "@progress/kendo-angular-charts": "^1.4.0",
    "@progress/kendo-angular-dateinputs": "^1.4.5",
    "@progress/kendo-angular-dialog": "^1.3.0",
    "@progress/kendo-angular-dropdowns": "^1.6.1",
    "@progress/kendo-angular-excel-export": "^1.0.5",
    "@progress/kendo-angular-grid": "^1.8.0",
    "@progress/kendo-angular-inputs": "^1.5.0",
    "@progress/kendo-angular-intl": "^1.3.0",
    "@progress/kendo-angular-l10n": "^1.0.5",
    "@progress/kendo-angular-label": "^1.0.5",
    "@progress/kendo-angular-layout": "^1.2.1",
    "@progress/kendo-angular-popup": "^1.3.4",
    "@progress/kendo-angular-scrollview": "^1.0.7",
    "@progress/kendo-angular-sortable": "^1.0.6",
    "@progress/kendo-angular-treeview": "^1.1.1",
    "@progress/kendo-angular-upload": "^1.2.2",
    "@progress/kendo-data-query": "^1.1.2",
    "@progress/kendo-drawing": "^1.4.2",
    "@progress/kendo-file-saver": "^1.0.4",
    "@progress/kendo-theme-bootstrap": "^2.11.5",
    "core-js": "^2.5.1",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.5.2",
    "shelljs": "^0.7.8",
    "wijmo": "file:vendors\\wijmo\\js",
    "wijmo-css": "file:vendors\\wijmo\\css",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.4.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "gulp-concat": "^2.6.1",
    "gulp4": "^4.0.2",
    "raw-loader": "^0.5.1",
    "typescript": "^2.6.1",
    "webpack": "^3.8.1"
